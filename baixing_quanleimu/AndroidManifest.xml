	<uses-permission android:name="android.permission.READ_USER_DICTIONARY"/>
	<uses-permission android:name="android.permission.WRITE_USER_DICTIONARY"/>
	<uses-permission android:name="android.permission.WAKE_LOCK"/>
	
	<application android:icon="@drawable/app_icon" android:label="@string/app_name" android:theme="@style/myTheme">
        <meta-data android:name="UMENG_APPKEY" android:value="4f7139155270154a7f000051">
        </meta-data>
        <meta-data android:name="UMENG_CHANNEL" android:value="androidmarket_umeng" />
        <meta-data android:name="publishTime" android:value="2012-09-29 11:05" />
        
        <activity android:configChanges="orientation|keyboardHidden|navigation" android:label="@string/app_name" android:name="BaiduMapActivity" android:theme="@style/lightTheme" android:screenOrientation="portrait" android:launchMode="singleTask">
        </activity>
        
<!-- 		<activity android:configChanges="orientation|keyboardHidden|navigation" android:label="@string/app_name" android:name=".ManagerActivity" android:theme="@style/lightTheme" android:screenOrientation="portrait" android:launchMode="singleInstance">
			<intent-filter>
			    <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
			<intent-filter>
			    <action android:name="switch" />
            </intent-filter>
        </activity> -->
        
        <activity android:configChanges="orientation|keyboardHidden|navigation" android:label="@string/app_name" android:name=".PersonalActivity" android:theme="@style/lightTheme" android:screenOrientation="portrait" android:launchMode="singleInstance">
			<intent-filter>
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        
        <activity android:configChanges="orientation|keyboardHidden|navigation" android:label="@string/app_name" android:name=".PostActivity" android:theme="@style/lightTheme" android:screenOrientation="portrait" android:launchMode="singleInstance">
            <intent-filter>
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        
        <activity android:configChanges="orientation|keyboardHidden|navigation" android:label="@string/app_name" android:name=".QuanleimuMainActivity" android:theme="@style/lightTheme" android:screenOrientation="portrait" android:launchMode="singleInstance" android:windowSoftInputMode="adjustResize">
           <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        
        <service
            android:name="com.quanleimu.broadcast.BXNotificationService"
            android:exported="false"
            android:enabled="true" >
        </service>
        
		<service
            android:name="com.quanleimu.broadcast.PushMessageService"
            android:exported="false"
            android:enabled="true" >
           <intent-filter>
                <action android:name="com.quanleimu.action.CONNECT" />
                <action android:name="com.quanleimu.action.DISCONNECT" />
                <action android:name="com.quanleimu.action.NETWORK_CHANGED" />
            </intent-filter>
        </service>

        <service
                android:name="com.umeng.common.net.DownloadingService"
                android:exported="true"
                android:process=":DownloadingService" >
        </service>

        <service
                android:name="com.quanleimu.util.BXUpdateService"
                android:label="AutoUpdateService"/>

        <receiver android:name="com.quanleimu.broadcast.NetworkConnectivityReceiver" android:enabled="true">
             <intent-filter>
                 <action android:name="android.net.conn.CONNECTIVITY_CHANGE" />
             </intent-filter>
        </receiver>
        
		<receiver android:name="com.quanleimu.broadcast.NotificationHandler" android:enabled="true">
             <intent-filter>
                 <action android:name="com.baixing.action.notify.msg" />
                 <action android:name="com.baixing.action.notify.bxinfo" />
                  <action android:name="com.baixing.action.notify.hot" />
             </intent-filter>
        </receiver>
        

        <receiver android:name="com.quanleimu.broadcast.BXStartupIntentReceiver" >
            <intent-filter>
                <action android:name="android.intent.action.BOOT_COMPLETED" />
                <action android:name="com.baixing.action.notify.upgrade" />
            </intent-filter>
        </receiver>
    </application>

</manifest>